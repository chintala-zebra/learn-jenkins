pipeline {
  agent any
  parameters {
    base64File 'THEFILE'
  }
  stages {
    stage('Example') {
      steps {
        script{
         sh "set +x; echo '$THEFILE' | base64 -d > ${WORKSPACE}/uploaded_file.txt"
         cat '${WORKSPACE}/uploaded_file.txt'
        }
      }
    }
  }
      post {
        always {
            cleanWs()
        }
    }

}