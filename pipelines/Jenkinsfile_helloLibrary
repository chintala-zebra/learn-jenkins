@Library('middleware-library')_
import org.zebra.Hello

pipeline {
    agent any
    options {
        ansiColor('xterm')
    }
    stages {
        stage('Print the Values') {
            steps {
                script {
                    
                    log.info('Starting')
                    sayHello('Dave')
                    log.warning('Nothing')
                    log.error('Failed!')
                    listFolders 'GCP-Sandbox'
                    def hello = new Hello()
                    log.info(hello.printHello("Chintala"))
                }
            }
        }
    }
}