@Library('middleware-library') _

pipeline {
    agent any
    options {
        ansiColor('xterm')
    }
    stages {
        stage('Print the Values') {
            steps {
                script {
                    log.info 'Starting'
                    sayHello 'Dave'
                    log.warning 'Nothing'
                    log.error 'Failed!'
                }
            }
        }
    }
}